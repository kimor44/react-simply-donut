{
  "name": "react-simply-donut",
  "author": "Kimor44",
  "private": false,
  "version": "1.0.1",
  "description": "A simple and customizable donut chart for React",
  "keywords": [
    "react",
    "donut",
    "chart",
    "pie",
    "circle",
    "doughnut",
    "simple",
    "customizable",
    "lightweight",
    "typescript"
  ],
  "homepage": "https://github.com/kimor44/react-simply-donut",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-simply-donut.cjs.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/react-simply-donut.es.js",
  "exports": {
    "./donuts": {
      "import": "./dist/donuts/react-simply-donut.es.js",
      "require": "./dist/donuts/react-simply-donut.cjs.js",
      "types": "./dist/donuts/index.d.ts"
    },
    "./legends": {
      "import": "./dist/legends/react-simply-donut.es.js",
      "require": "./dist/legends/react-simply-donut.cjs.js",
      "types": "./dist/legends/index.d.ts"
    },
    ".": {
      "import": "./dist/react-simply-donut.es.js",
      "require": "./dist/react-simply-donut.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "watch": "vite build --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.1",
    "vite-plugin-lib-inject-css": "^1.3.0"
  },
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kimor44/react-simply-donut.git"
  },
  "sideEffects": [
    "**/*.css"
  ]
}
